{
   "name": "millennio",
   "version": "1.0.0",
   "description": "Millennio is a Discord bot to help out in school..",
   "main": "src/index.ts",
   "scripts": {
      "build": "tsc",
      "watch": "tsc -w",
      "start": "npm run build && node -r dotenv/config dist/index.js dotenv_config_path=config/.env | pino-pretty -c -t SYS:standard",
      "slash": "ts-node -r dotenv/config scripts/slash_update.ts dotenv_config_path=config/.env",
      "scratch": "ts-node -r dotenv/config scripts/scratch.ts dotenv_config_path=config/.env",
      "format": "prettier --config .pretierrc 'src/**/*.ts' --write",
      "test": "echo \"Error: no test specified\" && exit 1"
   },
   "author": "Martín Zamorano Acuña",
   "license": "MIT",
   "dependencies": {
      "@discordjs/builders": "^0.8.1",
      "@discordjs/rest": "^0.1.0-canary.0",
      "@types/rimraf": "^3.0.2",
      "chalk": "^4.1.2",
      "date-fns": "^2.25.0",
      "discord-api-types": "^0.24.0",
      "discord-together": "^1.1.62",
      "discord.js": "^13.3.0",
      "dotenv": "^10.0.0",
      "form-data-encoder": "^1.7.1",
      "formdata-node": "^4.3.2",
      "lodash": "^4.17.21",
      "node-fetch": "^2.6.7",
      "node-schedule": "^2.0.0",
      "pino": "^7.11.0",
      "reflect-metadata": "^0.1.10",
      "sequelize": "^6.6.5",
      "turndown": "^7.1.1",
      "xmlhttprequest": "^1.8.0",
      "xmlhttprequest-ts": "^1.0.1"
   },
   "devDependencies": {
      "@babel/eslint-parser": "^7.15.4",
      "@types/lodash": "^4.14.176",
      "@types/node-schedule": "^1.3.2",
      "@types/turndown": "^5.0.1",
      "@typescript-eslint/eslint-plugin": "^5.22.0",
      "@typescript-eslint/parser": "^5.22.0",
      "chrono-node": "^2.3.2",
      "date-fns-timezone": "^0.1.4",
      "eslint": "^7.32.0",
      "eslint-config-airbnb-typescript": "^17.0.0",
      "eslint-config-prettier": "^8.5.0",
      "eslint-plugin-prettier": "^4.0.0",
      "fuse.js": "^6.4.6",
      "prettier": "^2.6.2",
      "rimraf": "^3.0.2",
      "ts-node": "^10.4.0",
      "tslib": "^2.3.1",
      "typescript": "^4.6.4"
   }
}
